{
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "semi": ["error", "always"],
    "max-len": ["error", { "code": 160 }],
    "no-console": ["error", { "allow": ["log", "warn", "error"] }],
    "object-curly-spacing": ["error", "always"],
    "camelcase": 0,
    "require-jsdoc": 0
  },
  "plugins": [],
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
